{
    "name": "sofyman/laravel-gettext",
    "keywords": ["laravel", "gettext"],
    "description": "Simple gettext loader and parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Lito",
            "email": "lito@eordes.com"
        }
    ],
    "require": {
        "php": ">=5.3",
        "gettext/gettext": "3.6.*"
    },
    "autoload": {
        "psr-4": {
            "Eusonlito\\LaravelGettext\\": "src/Eusonlito/LaravelGettext/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eusonlito\\LaravelGettext\\GettextServiceProvider"
            ],
            "aliases": {
                "Gettext": "Eusonlito\\LaravelGettext\\Facade"
            }
        }
    }
}
